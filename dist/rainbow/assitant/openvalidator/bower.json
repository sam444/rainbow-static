{
    "name": "openvalidator",
    "description": "The best jQuery plugin to validate form fields. Designed to use with Bootstrap 3",
    "version": "0.5.2",
    "ignore": [
        "demo",
        "screenshots",
        "test",
        "vendor",
        "*.md",
        "bootstrapValidator.jquery.json",
        "Gruntfile.js",
        "package.json"
    ],
    "main": [
        "dist/css/bootstrapValidator.css",
        "dist/js/bootstrapValidator.js"
    ],
    "license": "MIT",
    "keywords": ["jquery-plugin", "jQuery", "plugin", "validate", "validator", "form", "Bootstrap"],
    "authors": [
        { "name": "The OpenValidator Developers", "homepage": "https://github.com/OpenValidator/openvalidator" }
    ],
    "homepage": "https://github.com/OpenValidator/openvalidator",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenValidator/openvalidator.git"
    },
    "dependencies": {
        "jquery": ">= 1.9.0",
        "bootstrap": ">= 3.0.0"
    }
}
